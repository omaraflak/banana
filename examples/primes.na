fun isprime(n) {
    for (long i = 2; i < n; i++) {
        if (n % i == 0) {
            return 0;
        }
    }
    return 1;
}

fun main() {
    long n = 30;
    long i = 3;
    while (n > 1) {
        if (isprime(i) == 1) {
            print i;
            n--;
        }
        i += 2;
    }
}